    "ng-error": "^1.0.2",
    "node-forge": "~0.7.1",
    "uuid": "^3.0.0",
    "web-request-rpc": "^0.1.5"
  },
  "bedrock": {
    "browserDependencies": [
      "angular-stackables",
      "angular-web-request-mediator",
      "bedrock-angular",
      "bedrock-angular-bootstrap",
      "credentials-polyfill",
      "credential-mediator-polyfill",
      "did-io",
      "font-awesome",
      "jsonld-signatures",
      "ng-error",
      "node-forge",
      "uuid",
      "web-request-rpc"
    ],
    "systemjs": {
      "packages": {
        "node-forge": {
          "map": {
            "crypto": "@empty"
          }
        },
        "uuid": {
          "map": {
            "./lib/rng.js": "./lib/rng-browser.js"
          }
        }
      }
    },
    "manifest": {
      "authio": {
        "module": "index.js",
        "browser": "index.js"
      }
    }
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "directories": {}
}
